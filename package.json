{
  "name": "hypothesize",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run client-dev\" \"npm run api-dev\"",
    "client-dev": "better-npm-run client-dev",
    "api-dev": "better-npm-run api-dev",
    "watch-client": "better-npm-run watch-client"
  },
  "betterScripts": {
    "client-dev": {
      "command": "next -p 1773",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "api-dev": {
      "command": "nodemon api/index.js --debug --watch ./api",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "PORT": 1774
      }
    }
  },
  "author": "Matthew Frey <matthewfrey118@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "better-npm-run": "^0.0.13",
    "concurrently": "^3.1.0",
    "express": "^4.14.0",
    "next": "^1.1.1"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  }
}
